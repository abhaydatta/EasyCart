    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_screen_bg"
    tools:context=".activity.GorceerCategoryActivity">


    <include
        android:id="@+id/include_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/layout_top_categorylist" />

    <GridView
        android:id="@+id/itemListGridView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:verticalSpacing="5dp"
        android:horizontalSpacing="5dp"
        android:numColumns="auto_fit"
        android:padding="5dp"
        app:layout_constraintTop_toBottomOf="@id/include_top"/>
